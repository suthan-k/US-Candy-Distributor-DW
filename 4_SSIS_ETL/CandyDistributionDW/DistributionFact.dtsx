            // Convert degrees to radians for the calculation.
            double factoryLatRad = (System.Math.PI / 180.0) * Row.FactoryLatitude;
            double customerLatRad = (System.Math.PI / 180.0) * Row.CustomerLatitude;
            double deltaLat = (System.Math.PI / 180.0) * (Row.CustomerLatitude - Row.FactoryLatitude);
